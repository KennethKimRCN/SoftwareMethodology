<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.photos_android.photos_android63">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="Albums"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.photos_android.photos_android63.UserHomepage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.photos_android.photos_android63.SingleAlbumPage"
            android:parentActivityName="com.photos_android.photos_android63.UserHomepage" />
        <activity
            android:name="com.photos_android.photos_android63.Slideshow"
            android:parentActivityName="com.photos_android.photos_android63.SingleAlbumPage" />
        <activity android:name="com.photos_android.photos_android63.photosInAlbum_list" />
        <activity
            android:name="com.photos_android.photos_android63.SearchPage"
            android:parentActivityName="com.photos_android.photos_android63.UserHomepage" />
        <activity android:name="com.photos_android.photos_android63.PhotoTagsListView" />
        <activity android:name="com.photos_android.photos_android63.photosInSearchedListView"></activity>
    </application>

</manifest>